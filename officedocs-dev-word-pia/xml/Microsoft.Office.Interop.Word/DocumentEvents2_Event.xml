    <Member MemberName="ContentControlOnEnter">
      <MemberSignature Language="C#" Value="event Microsoft.Office.Interop.Word.DocumentEvents2_ContentControlOnEnterEventHandler ContentControlOnEnter;" />
      <MemberSignature Language="ILAsm" Value=".event class Microsoft.Office.Interop.Word.DocumentEvents2_ContentControlOnEnterEventHandler ContentControlOnEnter" />
